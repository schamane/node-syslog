# EditorConfig is awesome: http://EditorConfig.org
root = true

[*]
end_of_line = lf
indent_size = 2
indent_style = space
trim_trailing_whitespace = true
charset = utf-8
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false

[package.json]
indent_size = 2
insert_final_newline = false

[*.json]
indent_size = 2
insert_final_newline = false
