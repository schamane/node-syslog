{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "src/index.ts"
  ],
  "out": "docs/api",
  "readme": "none",
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "hideGenerator": true,
  "sort": ["source-order"],
  "githubPages": false,
  "theme": "default",
  "name": "node-syslog API",
  "includeVersion": false,
  "disableSources": false,
  "sourceLinkTemplate": "https://github.com/schamane/node-syslog/blob/main/{path}#L{line}",
  "gitRevision": "main"
}